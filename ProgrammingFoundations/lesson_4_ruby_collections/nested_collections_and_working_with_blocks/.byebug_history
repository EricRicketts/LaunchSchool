sub_array.map {|elem|  elem }
sub_array.map {|elem| puts elem; elem }
sub_array.map do |elem|
sub_array.map {|elem| puts elem; puts -elem; elem }
sub_array.map {|elem| -elem }
sub_array.sort_by {|elem| '-' + elem}
sub_array.sort_by {|elem| elem}
sub_array.sort_by {|elem| -elem}
